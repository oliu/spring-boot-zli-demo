<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.formicine.weaver.dao.mapper.BsAdvertMapper">
    <select id="selectById" resultType="com.formicine.weaver.domain.model.BsAdvert">
        SELECT
            *
        FROM
            bs_advert
        WHERE
            id = #{id,jdbcType=BIGINT}
    </select>
</mapper>